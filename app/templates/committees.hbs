<h1>Committees</h1>
<div class="row">
  <div class="col-sm-6">
    <h3>House</h3>
    <ul>
      {{#each model.house as |hcommittee|}}
        <li>{{#link-to 'committee' hcommittee.committee_id}}{{hcommittee.name}}{{/link-to}}</li>
      {{/each}}
    </ul>
  </div>
  <div class="col-sm-6">
    <h3>Senate</h3>
    <ul>
      {{#each model.senate as |scommittee|}}
        <li>{{#link-to 'committee' scommittee.committee_id}}{{scommittee.name}}{{/link-to}}</li>
      {{/each}}
    </ul>
  </div>
</div>
